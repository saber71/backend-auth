{
  "name": "@heraclius/backend-auth",
  "version": "1.3.0",
  "private": "true",
  "description": "提供登录授权和token校验服务。它实现了用户登录、注册、密码加密存储、生成JWT令牌以及令牌有效性校验",
  "type": "module",
  "scripts": {
    "build": "pipenv run pyinstaller main.py -y",
    "start": "pipenv run uvicorn main:app --port 10002 --reload",
    "test": "vitest run"
  },
  "devDependencies": {
    "@heraclius/backend-bridge": "^1.1.0",
    "@heraclius/external-deps-plugin": "^1.1.0",
    "@heraclius/http-test": "^1.1.0",
    "@types/node": "^20.14.11",
    "unplugin-swc": "^1.5.1",
    "vite": "^5.3.4",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.0.3"
  },
  "peerDependencies": {
    "@heraclius/backend-bridge": "^1.0.0"
  }
}
